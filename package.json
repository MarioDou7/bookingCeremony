{
  "name": "bookingceremonies",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "server:dev": " nodemon ./src/bin",
    "server:build": " node ./src/bin",
    "client:dev": "cd client && yarn start",
    "client:build": "cd client && yarn && yarn build",
    "dev": "SET NODE_ENV=development && concurrently --kill-others-on-fail \"yarn server:dev\" \"yarn client:dev\"",
    "start": "node ./src/bin",
    "heroku-postbuild": "yarn client:build"
  },
  "dependencies": {
    "axios": "^0.19.2",
    "cookie-parser": "~1.4.4",
    "debug": "~2.6.9",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "http-errors": "~1.6.3",
    "i18n": "^0.10.0",
    "joi": "^14.3.1",
    "mongod": "^2.0.0",
    "mongoose": "^5.9.15",
    "morgan": "~1.9.1"
  },
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-loader": "^8.1.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "concurrently": "^5.2.0",
    "nodemon": "^2.0.4"
  }
}